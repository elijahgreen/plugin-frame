{
  "name": "plugin-frame",
  "license": "MIT",
  "version": "0.6.0",
  "homepage": "https://github.com/elijahgreen/plugin-frame",
  "author": "Elijah Green",
  "type": "module",
  "main": "./dist/plugin-frame.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:browser": "vitest --workspace=vitest.workspace.ts",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.13.9",
    "@vitest/browser": "^3.2.4",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "happy-dom": "^20.0.11",
    "playwright": "^1.50.1",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elijahgreen/plugin-frame.git"
  },
  "bugs": {
    "url": "https://github.com/elijahgreen/plugin-frame/issues"
  },
  "keywords": [
    "sandbox",
    "web",
    "plugin"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
